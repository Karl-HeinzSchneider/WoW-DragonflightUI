				</Anchors>
			</CheckButton>
			<CheckButton name="$parentButton6" inherits="DFMultiBar5ButtonTemplate" id="6">
				<Anchors>
					<Anchor point="TOP" relativeTo="$parentButton5" relativePoint="BOTTOM" x="0" y="-6" />
				</Anchors>
